<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Hacn</PackageId>
    <Version>0.0.6</Version>
    <Authors>Paul Johnson</Authors>
    <Description>DSL for creating React components using Fable and F# computation expressions</Description>
    <RepositoryUrl>https://github.com/pj/hacn</RepositoryUrl>
    <PackageIconUrl></PackageIconUrl>
    <PackageTags>fsharp;fable;react;feliz</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="*.fsproj; *.fs; *.js" PackagePath="fable\" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="Types.fs" />
    <Compile Include="Operations.fs" />
    <Compile Include="Core.fs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Expecto" Version="9.0.2" />
    <PackageReference Include="Fable.Browser.Dom" Version="1.1.0" />
    <PackageReference Include="Fable.Core" Version="3.1.5" />
    <PackageReference Include="Fable.React" Version="6.2.0" />
    <PackageReference Include="Feliz" Version="1.17.0" />
    <PackageReference Include="FSharp.Interop.Dynamic" Version="5.0.1.268" />
  </ItemGroup>
</Project>