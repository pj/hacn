{
  "private": true,
  "scripts": {
    "test:e2e:server": "webpack-dev-server --config ./e2etests/webpack.config.js",
    "test:e2e:cypress": "cypress open -C e2etests/cypress.json",
    "prejstest": "fable-splitter jstests -o dist/jstests --commonjs -c jstests/splitter.config.js",
    "jstest": "mocha --require jsdom-global/register dist/jstests/*.test.js",
    "test": "dotnet run --project=tests/Test.fsproj",
    "build:core:tests": "dotnet build tests/Test.fsproj"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "cypress": "^5.4.0",
    "fable-compiler": "^2.13.0",
    "fable-loader": "^2.1.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@sinonjs/fake-timers": "^6.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.4",
    "fable-splitter": "^2.2.1",
    "jest": "^26.4.2",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.1.3",
    "prettier": "^2.1.1"
  },
  "jest": {
    "roots": [
      "./dist/jstests"
    ]
  }
}
